{
  "name": "mdast-util-from-quill-delta",
  "version": "0.0.0",
  "description": "utility to transform Quill delta to mdast",
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .nyc_output coverage dist",
    "format": "prettier --write \"src/*.{ts}\" \"./*.{json,md}\" && remark . -qfo && xo --fix",
    "prepublishOnly": "npm run clean && npm test && npm run build",
    "test": "nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syntax-tree/mdast-util-from-quill-delta.git"
  },
  "keywords": [
    "syntax-tree",
    "mdast",
    "quill",
    "mdast-util",
    "quill-delta",
    "markdown"
  ],
  "author": "Christian Murphy <christian.murphy.42@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syntax-tree/mdast-util-from-quill-delta/issues"
  },
  "homepage": "https://github.com/syntax-tree/mdast-util-from-quill-delta#readme",
  "dependencies": {
    "@types/mdast": "^3.0.3",
    "quill-delta": "^4.2.1",
    "unist-builder": "^1.0.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "ava": "^2.4.0",
    "eslint-config-xo-typescript": "^0.19.0",
    "husky": "^3.0.9",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "remark-cli": "^7.0.0",
    "remark-preset-wooorm": "^6.0.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4",
    "xo": "^0.25.3"
  },
  "ava": {
    "compileEnhancements": false,
    "files": [
      "src/**/*.test.ts"
    ],
    "sources": [
      "src/**/*"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "extends": "xo-typescript",
    "extensions": [
      "ts"
    ],
    "rules": {
      "@typescript-eslint/semi": "off",
      "@typescript-eslint/indent": "off",
      "import/default": "off"
    },
    "ignores": [
      "dist",
      ".nyc_output",
      "coverage"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  }
}
